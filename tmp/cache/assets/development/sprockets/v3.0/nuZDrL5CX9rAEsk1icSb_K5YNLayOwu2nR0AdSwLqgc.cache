I"l(function() {
  $(function() {
    var $container1;
    $container1 = $("#container1");
    return $container1.imagesLoaded(function() {
      return $container1.masonry({
        itemSelector: ".item",
        columnWidth: ".grid-sizer"
      });
    });
  });

  $(function() {
    return $('#pictureframes').imagesLoaded(function() {
      return $('#pictureframes').masonry({
        itemSelector: '.box',
        isFitWidth: true,
        percentPosition: true
      });
    });
  });

  $(function() {
    if ($('#infinite-scrolling').size() > 0) {
      $(window).on('scroll', function() {
        var more_pins_url;
        more_pins_url = $('.pagination a[rel=next]').attr('href');
        if (more_pins_url && $(window).scrollTop() > $(document).height() - $(window).height() - 60) {
          $('.pagination').html("");
          $.ajax({
            url: more_pins_url,
            success: function(data) {
              return $("#container1").append(data);
            }
          });
        }
        if (!more_pins_url) {
          return $('.pagination').html("");
        }
      });
    }
  });

}).call(this);
